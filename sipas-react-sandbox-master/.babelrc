{
	"presets": ["@babel/preset-flow"],
	"plugins": [
		"transform-flow-strip-types",
		["@babel/plugin-proposal-decorators", { "legacy": true }],
		["@babel/plugin-proposal-class-properties", { "loose": true }],
		[
			"@babel/plugin-transform-runtime",
			{
				"helpers": true,
				"regenerator": true
			}
		]
	]
}
